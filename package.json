{
  "name": "simple-code-cleaner",
  "displayName": "Simple Code Cleaner",
  "description": "Simple VScode extension to clean flutter (soon more langs) code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onCommand:extension.removeExtraCommas",
    "onCommand:extension.flattenSelectedFunction"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.removeExtraCommas",
        "title": "Remove Extra Commas Before Brackets"
      },
      {
        "command": "extension.flattenSelectedFunction",
        "title": "Flatten Selected Function"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codeflane/simple-code-cleaner.git"
  }
}